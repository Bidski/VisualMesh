{
    "python.formatting.provider": "yapf",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.eol": "\n",
    "editor.rulers": [
        120
    ],
    "cSpell.words": [
        "EQUISOLID",
        "convolutional"
    ],
    "files.associations": {
        "__split_buffer": "cpp",
        "list": "cpp",
        "vector": "cpp",
        "__bit_reference": "cpp",
        "__string": "cpp",
        "__tree": "cpp",
        "string_view": "cpp",
        "map": "cpp",
        "set": "cpp",
        "string": "cpp"
    },
}
